ENV_FILE=.env.staging

# DB credentials
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=microloans_staging
DB_PORT=5432

# API
API_PORT=443
FLASK_ENV=staging
LOG_LEVEL=info

# Database URL
DATABASE_URL=postgresql+psycopg2://postgres:postgres@db:5432/microloans_staging

# Staging-specific settings
ENABLE_DEBUG_TOOLBAR=false
GUNICORN_WORKERS=4
GUNICORN_TIMEOUT=90
